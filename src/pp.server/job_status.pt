<html>
    <head>
        <link rel="stylesheet" href="/static/webservice.css" />
    </head>
    <body>
        <h1>Job ${job.job_id}</h1>
        <table border="1">
            <head>
                <tr>          
                    <th>Key</th>
                    <th>Value</th>
                </tr>
            </head>
            <tbody>
                <tr tal:repeat="key sorted(job)">
                    <td>${key}</td>
                    <td tal:content="structure view.cr2br(job[key])" />
                </tr>
            </tbody>
        </table>
    </body>
</html>
